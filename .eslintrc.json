{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}